{
    "env": {
        "commonjs": true,
        "node": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": ["warn"],
        "no-console": "off",
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }
}
